{
    "sources": {
        "neighborhoods": {
            "type": "geojson",
            "data": {
                "type": "FeatureCollection",
                "features": []
            }
        }
    },
    "layers": [
        {
            "id": "neighborhoods-outline",
            "type": "line",
            "source": "neighborhoods",
            "paint": {
                "line-width": 3
            },
            "filter": [
                "!=",
                [
                    "get",
                    "count"
                ],
                0
            ]
        },
        {
            "id": "neighborhoods-fill",
            "type": "fill-extrusion",
            "source": "neighborhoods",
            "paint": {
                "fill-extrusion-color": [
                    "step",
                    ["get", "count"],
                    "#f1eef6",
                    50, "#d0d1e6",
                    100, "#a6bddb",
                    300, "#74a9cf",
                    600, "#2b8cbe",
                    1000, "#045a8d"
                ],
                "fill-extrusion-opacity": 0.8,
                "fill-extrusion-height": [
                    "*",
                    ["get", "count"],
                    5
                ]
            },
            "filter": [
                "!=",
                ["get", "count"],
                0
            ]
        }
    ]
}
